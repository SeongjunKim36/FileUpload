{
  "name": "demo-microservice-boilerplate",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript boilerplate repository",
  "author": "Med Ghaith Sellami <medghaith.sellami@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0xb4lamx/nestjs-boilerplate-microservice.git"
  },
  "bugs": {
    "url": "https://github.com/0xb4lamx/nestjs-boilerplate-microservice/issues"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "release": "npx -p @semantic-release/changelog@^5.0.0 -p @semantic-release/git@^9.0.0  -p semantic-release@^17.0.0 semantic-release --no-ci --debug",
    "commit": "npx git-cz",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "ts-node node_modules/typeorm/cli.js migration:generate -f ormconfig -d src/migrations -n",
    "migration:revert": "ts-node node_modules/typeorm/cli.js migration:revert -f ormconfig",
    "migration:create": "ts-node node_modules/typeorm/cli.js migration:create -f ormconfig -d src/migrations -n",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -f ormconfig",
    "schema:drop": "ts-node node_modules/typeorm/cli.js schema:drop -f ormconfig",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nest start --watch",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "start:prod": "node dist/main.js",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts'",
    "lint:fix": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "swagger": "NODE_ENV=swagger npx nestia swagger --config nestia.config.ts --project tsconfig.json",
    "start:swagger": "docker-compose up -d && NODE_ENV=swagger yarn swagger && ts-node executable/swagger.ts --skipBuild",
    "swagger:generate": "nestia swagger",
    "swagger:dev": "nestia swagger --watch",
    "prepare": "ts-patch install && typia patch"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.421.0",
    "@aws-sdk/s3-request-presigner": "^3.421.0",
    "@nestia/core": "^4.5.2",
    "@nestia/e2e": "^0.7.0",
    "@nestia/fetcher": "^4.5.2",
    "@nestia/sdk": "^4.5.2",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.3.7",
    "@nestjs/swagger": "^7.1.2",
    "@nestjs/typeorm": "^10.0.2",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/sharp": "^0.31.1",
    "aws-sdk": "^2.1419.0",
    "axios": "^1.6.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.14.0",
    "dotenv": "^8.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "lodash": "^4.17.21",
    "mime-types": "~2.1.24",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.9.8",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.5",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.20",
    "typia": "^7.5.1",
    "uuid": "^9.0.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.10.0",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.0.0",
    "@types/lodash": "^4.14.134",
    "@types/mime-types": "^2.1.0",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.4.12",
    "@types/node": "^16.14.0",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.0.0",
    "nestia": "^6.3.1",
    "prettier": "^3.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.0",
    "ts-morph": "^19.0.0",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.3.0",
    "tsc-watch": "^6.0.4",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
